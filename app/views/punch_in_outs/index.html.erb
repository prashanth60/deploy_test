<p id="notice"><%= notice %></p>

<h1>Listing Punch In Outs</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Check in</th>
      <th>Check out</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @punch_in_outs.each do |punch_in_out| %>
      <tr>
        <td><%= punch_in_out.user_id %></td>
        <td><%= punch_in_out.check_in %></td>
        <td><%= punch_in_out.check_out %></td>
        <td><%= link_to 'Show', punch_in_out %></td>
        <td><%= link_to 'Edit', edit_punch_in_out_path(punch_in_out) %></td>
        <td><%= link_to 'Destroy', punch_in_out, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Punch in out', new_punch_in_out_path %>
